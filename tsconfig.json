{
  "compilerOptions": {
    "jsx": "react",
    "jsxImportSource": "@emotion/react",
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2019"
    ],
    "paths": {
      "@elastic/eui/es/components/icon/*": ["./src/assets.d.ts"],
      "@elastic/eui/dist/*": ["./src/assets.d.ts"]
    }
  },
  "include": [
    "src/**/*"
  ]
}
